#include<iostream>
int main()










bool checkFood(char s[][40],int fx,int fy)
{
  int x,y,c=0;
  for(x=1;x<49;x++)
  {
     for(y=1;y<39;y++)
     {
        if(s[x][y]=='o' && x==fx && y==fy)
        {
           c=1;
        }
     }
  }
  if(c==1) return true;
  else return false;
}

















































void table(char s[][40],int score)
{
    int x=50;
    int y=40;
    int i,j;
    for(i=0;i<y;i++)
    {
        for(j=0;j<x;j++)
        {
            if(j<(x-1)) cout<<s[j][i];
            if(j==(x-1)) cout<<s[j][i]<<endl;
        }
    }
    cout<<""<<endl;
    cout<<"Diem cua ban: "<<score;
}

























































































































































































































































































































































































































































